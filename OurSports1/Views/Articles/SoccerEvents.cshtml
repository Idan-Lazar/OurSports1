@model IEnumerable<OurSports1.Models.Article>

<h2>Soccer Events </h2>

<nav class="navbar navbar-light bg-light Searchline">

    <form class="form-inline Searchline Archive" asp-controller="Articles" asp-action="SoccerEvents">
        <p>

            <div class="form-check">
                <input class="form-check-input" type="checkbox" value="true" id="DateFrom" >
                Date From: <input type="date" class="form-control mr-sm-2 disabled" name="Date From" value="From">
            </div>
            <div class="form-check">
                <input class="form-check-input" type="checkbox" value="true" id="DateTo" >
                Date To: <input type="date" class="form-control mr-sm-2 disabled" name="Date To" value="To">
            </div>
            <div class="form-check">
                <input class="form-check-input" type="checkbox" value="true" id="League">
                League:<select class="form-control mr-sm-2" value="League" name="League">
    <option value="437">Israel</option>
    <option value="62">England</option>
    <option value="109">Spain</option>

</select>
            </div>
           
            <button type="submit" class="btn btn-outline-success font-weight-bold " Text="Filter">Filter</button>

        </p>
    </form>

    <div>
        <table border-collapse: collapse border: 5px solid black;>
            <tr>
                <th>England</th>
                <th>Spain</th>
                <th>Israel</th>

            </tr>
            <tr>
                <th> <table class="containerjson"></table> </th>
                <th> <table class="container1json"></table> </th>
                <th> <table class="container2json"></table> </th>
            </tr>

        </table>


    </div>


